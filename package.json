  "scripts": {
    "dev": "tsx watch src .env | pino-pretty --colorize",
    "buildo": "npx tsc",
    "build": "esbuild `find src \\( -name '*.ts' \\)` --platform=node --outdir=build --resolve-extensions=.js",
    "start": "node build",
    "start0": "node dist/index.js",
    "devo": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check:types": "tsc --noEmit",
    "pretest": "npm run build",
    "test": "vitest",
    "test:watch": "vitest -w"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.26.8",
    "avj": "^0.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "concurrently": "^8.0.1",
    "esbuild": "^0.14.54",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.22",
    "pino-pretty": "^10.0.0",
    "prettier": "^2.8.0",
    "tsx": "^3.12.1",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
